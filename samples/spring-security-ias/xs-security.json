{
	"xsappname": "ks1-q4",
	"oauth2-configuration": {
		"grant-types":["client_x509","urn:ietf:params:oauth:grant-type:jwt-bearer"]
	},
	"scopes"        : [
		{
			"name"        : "$XSAPPNAME.Admin",
			"description" : "Admin permissions."
		}
	],
	"role-templates": [
		{
			"name"                 : "Administrator",
			"description"          : "View Sensitive Data",
			"scope-references"     : [
				"$XSAPPNAME.Admin"
			]
		}
	],
	"role-collections": [
		{
			"name": "Administrator",
			"description": "Administrator (read all)",
			"role-template-references": [
				"$XSAPPNAME.Administrator"
			]
		}
	]
}
